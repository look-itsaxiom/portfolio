# Gemini API (required for Ask Axiom chat + embeddings)
GEMINI_API_KEY=your_gemini_api_key
GEMINI_MODEL=gemini-2.5-flash

# Discord Bot (required for fallback flow)
DISCORD_BOT_TOKEN=your_discord_bot_token
DISCORD_USER_ID=your_discord_user_id

# Optional overrides
QDRANT_COLLECTION=ask-axiom-knowledge

# Admin Dashboard
ADMIN_SECRET=your_admin_secret_here

# Revalidation (shared between web and admin for on-demand cache busting)
REVALIDATE_SECRET=your_revalidate_secret_here
